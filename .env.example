# Discord Bot Token
DISCORD_BOT_TOKEN=your-bot-token-here

# Discord Application ID
DISCORD_APPLICATION_ID=your-application-id-here

# Default Discord Webhook
DISCORD_WEBHOOK=https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN

# Additional Webhooks (optional)
DISCORD_WEBHOOK_ALERTS=https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN
DISCORD_WEBHOOK_BUILDS=https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN
DISCORD_WEBHOOK_ERRORS=https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN

# Logging
DISCORD_LOG_LEVEL=info  # debug, info, warn, error
